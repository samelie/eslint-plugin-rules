{
    "name": "@rad/eslint-plugin-rules",
    "version": "0.0.0",
    "keywords": [
        "eslint",
        "eslintplugin",
        "eslint-plugin"
    ],
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        }
    },
    "main": "./dist/index.mjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "unbuild",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "types": "tsc -p tsconfig.typecheck.json"
    },
    "dependencies": {
        "tsup": "^8.4.0"
    },
    "devDependencies": {
        "@types/eslint": "^9.6.1",
        "@typescript-eslint/utils": "^8.30.1",
        "eslint": "^9.25.0",
        "unbuild": "^3.5.0"
    }
}
