{
  "name": "@adddog/eslint-plugin-rules",
  "version": "0.0.2",
   "repository": {
    "type": "git",
    "url": "https://github.com/samelie/eslint-plugin-rules.git"
  },
  "bugs": {
    "url": "https://github.com/samelie/eslint-plugin-rules/issues"
  },
  "homepage": "https://github.com/samelie/eslint-plugin-rules#readme",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "knip": "knip",
    "test": "vitest run",
    "prepublishOnly": "pnpm run test && pnpm run lint && pnpm run types && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "release": "bumpp",
    "dev": "tsx src/index.ts"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@typescript-eslint/utils": "^8.46.0",
    "typescript": "^5.9.3",
    "bumpp": "^10.3.1",
    "unbuild": "^3.6.1"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "dependencies": {
    "eslint": "^9.37.0"
  },
  "peerDependencies": {
    "typescript": "^5.9.3",
    "knip": "^5.64.3",
    "vitest": "^3.2.4"
  }
}
