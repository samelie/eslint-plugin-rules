{
  "name": "@rad/eslint-plugin-rules",
  "version": "0.0.0",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json"
  },
  "dependencies": {
    "tsup": "^8.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/utils": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "unbuild": "^3.5.0"
  }
}
