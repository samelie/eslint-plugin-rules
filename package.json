{
  "name": "@rad/eslint-plugin-rules",
  "version": "0.0.0",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "knip": "knip"
  },
  "devDependencies": {
    "@rad/config": "workspace:*",
    "@rad/eslint": "workspace:*",
    "@typescript-eslint/utils": "^8.45.0",
    "eslint": "^9.37.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  }
}
