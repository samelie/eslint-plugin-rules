{
  "name": "@adddog/eslint-plugin-rules",
  "type": "module",
  "version": "0.0.10",
  "private": false,
  "homepage": "https://github.com/samelie/eslint-plugin-rules#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/samelie/eslint-plugin-rules.git"
  },
  "bugs": {
    "url": "https://github.com/samelie/eslint-plugin-rules/issues"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">= 22",
    "pnpm": ">= 10",
    "npm": ">= 11"
  },
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "types": "tsc -p tsconfig.typecheck.json",
    "knip": "knip",
    "test": "echo '⚠️⚠️⚠️no test vitest --watch=false'",
    "prepublishOnly": "pnpm run lint && pnpm run types && pnpm run build",
    "prepack": "pnpm run prepublishOnly",
    "release": "bumpp --yes --no-push --no-tag --release patch",
    "dev": "tsx src/index.ts"
  },
  "peerDependencies": {
    "knip": "^5.64.3",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@eslint/js": "^9.39.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@typescript-eslint/utils": "^8.52.0",
    "bumpp": "^10.3.2",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
